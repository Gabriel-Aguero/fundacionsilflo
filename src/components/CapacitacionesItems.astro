---
export interface Props {
  category: string;
  title: string;
  description: string;
  images: string;
}

const { category, title, description, images } = Astro.props;
---

<a href="#" class="block">
  <img alt="" src={images} class="h-64 w-96 object-cover sm:h-60" />

  <h3 class="mt-4 text-lg font-bold text-gray-900 sm:text-xl">
    {title}
  </h3>

  <p class="mt-2 max-w-sm text-gray-700">
    {description}
  </p>
</a>
